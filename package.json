{
  "name": "ts-plugin",
  "version": "0.1.3",
  "description": "Easy installs for TypeScript plugins",
  "main": "dist/index.js",
  "scripts": {
    "minify": "uglifyjs --compress --mangle --toplevel --output dist/index.js -- dist/index.js",
    "build": "tsc && rollup -c && npm run minify && rm -rf ./dist/src/",
    "start": "node dist/index.js",
    "dev": "npm run build && npm start",
    "version": "npm build"
  },
  "files": [
    "dist/index.js"
  ],
  "bin": {
    "ts-plugin": "dist/index.js"
  },
  "devDependencies": {
    "@types/cli-spinner": "^0.2.0",
    "@types/node": "^10.7.1",
    "cli-spinner": "^0.2.8",
    "got": "^9.0.0",
    "is-scoped": "^1.0.0",
    "registry-url": "^4.0.0",
    "rollup": "^0.64.1",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "turbocolor": "^2.6.1",
    "typescript": "^3.0.1",
    "uglify-es": "^3.3.9"
  },
  "author": "Nate Moore",
  "contributors": [
    {
      "name": "Nate Moore",
      "email": "nate@natemoo.re",
      "url": "https://projects.natemoo.re"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "prompts": "^0.1.14",
    "uninstall": "0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natemoo-re/ts-plugin.git"
  },
  "keywords": [
    "typescript",
    "ts",
    "plugin",
    "installer",
    "manager"
  ]
}
